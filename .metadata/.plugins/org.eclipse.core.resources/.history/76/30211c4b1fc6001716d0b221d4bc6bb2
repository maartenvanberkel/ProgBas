package be.pxl.h6.exoef4;

public class H6ExOef4 {

	public static void main(String[] args) {
		StringBuilder tekst = new StringBuilder();
		char karakter = 'a';
		
		for (int i = 0; i < 26; i++) {
			tekst.insert(i, karakter);
			karakter++;
			
			if(tekst.charAt(i) % 2 == 0) {
				karakter += 26;
			}
		}
		System.out.println(tekst);
	}
}